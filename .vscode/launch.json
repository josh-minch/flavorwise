        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "DEBUG": "False"
            },
            "args": [
                "run",
                "--no-debugger",
                "--host=0.0.0.0",
            ],
            "jinja": true
        }
    ]
